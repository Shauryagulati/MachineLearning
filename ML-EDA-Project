        "id": "ec206c8d"
      },
      "source": [
        "#Dropping the outlier rows with standard deviation\n",
        "factor = 3\n",
        "upper_lim = dataset['player_height'].mean () + dataset['player_height'].std () * factor\n",
        "lower_lim = dataset['player_height'].mean () - dataset['player_height'].std () * factor\n",
        "\n",
        "dataset = dataset[(dataset['player_height'] < upper_lim) & (dataset['player_height'] > lower_lim)]"
      ],
      "id": "ec206c8d",
